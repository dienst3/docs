{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Dienst 3",
  "openapi": "openapi.yaml",
  "api": {
    "auth": {
      "inputPrefix": "Bearer"
    }
  },
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#6c00ff",
    "light": "#6c00ff",
    "dark": "#ffab42",
    "background": {
      "dark": "#1d232a",
      "light": "#f3f3f3"
    },
    "anchors": {
      "from": "#6c00ff",
      "to": "#ffab42"
    }
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dienst3.nl"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "routes"
    }
  ],
  "anchors": [
    {
      "name": "Dienst 3",
      "icon": "user-group",
      "url": "https://dienst3.nl"
    },
    {
      "name": "GitHub",
      "icon": "github",
      "url": "https://github.com/dienst3"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["routes/gettingstarted", "routes/errors"]
    },
    {
      "group": "Schemes",
      "pages": ["routes/scheme/account"]
    },
    {
      "group": "Account",
      "pages": ["routes/account/get", "routes/account/patch"]
    }
  ],
  "footerSocials": {
    "website": "https://dienst3.nl",
    "github": "https://github.com/dienst3"
  }
}
